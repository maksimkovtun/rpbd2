<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.test2.DAO.ClassEntity" table="class_" schema="public" catalog="rpdb_lab1">
        <id name="clId">
            <column name="cl_id" sql-type="integer"/>
        </id>
        <property name="clName">
            <column name="cl_name" sql-type="varchar" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>