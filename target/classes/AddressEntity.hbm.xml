<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.test2.DAO.AddressEntity" table="address" schema="public" catalog="rpdb_lab1">
        <id name="adId">
            <column name="ad_id" sql-type="integer"/>
        </id>
        <property name="adName">
            <column name="ad_name" sql-type="varchar" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>